    \entry{walkerApplicationReinforcementLearning2000}{article}{}{}
      \name{author}{1}{}{%
        {{hash=e8f5c90edaf8933b2c35a024ae7ff170}{%
           family={Walker},
           familyi={W\bibinitperiod},
