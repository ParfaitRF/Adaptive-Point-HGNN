    \entry{chenFastExactFixedradius2024}{online}{}{}
      \name{author}{2}{}{%
        {{hash=058f14a832420dac69087fd1a80298b3}{%
           family={Chen},
           familyi={C\bibinitperiod},
           given={Xinye},
