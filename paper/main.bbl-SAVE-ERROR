      \strng{namehash}{385ed7b9bb136adc1904cba4fac93954}
      \strng{fullhash}{385ed7b9bb136adc1904cba4fac93954}
      \strng{fullhashraw}{385ed7b9bb136adc1904cba4fac93954}
      \strng{bibnamehash}{385ed7b9bb136adc1904cba4fac93954}
